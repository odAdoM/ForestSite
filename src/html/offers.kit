                        <button class="std-choice-btn">Wybierz</button>
                    </div>


                </div>

                <table>
                    <thead>
                        <tr>
                            <th>Opcja</th>
                            <th>Oferta A</th>
                            <th>Oferta B</th>
                            <th>Oferta C</th>
                            <th>Oferta D</th>
                            <th>Oferta E</th>
                            <th>Wybór</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Funkcja 1</td>
                            <td class="plus">+</td>
                            <td class="minus">-</td>
                            <td class="plus">+</td>
                            <td class="plus">+</td>
                            <td class="minus">-</td>
                            <td><button class="select-btn">Wybieram</button></td>
                        </tr>
                        <tr>
                            <td>Funkcja 2</td>
                            <td class="minus">-</td>
                            <td class="plus">+</td>
                            <td class="plus">+</td>
                            <td class="minus">-</td>
                            <td class="plus">+</td>
                            <td><button class="select-btn">Wybieram</button></td>
                        </tr>
                        <tr>
                            <td>Funkcja 3</td>
                            <td class="plus">+</td>
                            <td class="plus">+</td>
                            <td class="minus">-</td>
                            <td class="plus">+</td>
                            <td class="plus">+</td>
                            <td><button class="select-btn">Wybieram</button></td>
                        </tr>
                        <tr>
                            <td>Funkcja 4</td>
                            <td class="minus">-</td>
                            <td class="plus">+</td>
                            <td class="plus">+</td>
                            <td class="plus">+</td>
                            <td class="minus">-</td>
                            <td><button class="select-btn">Wybieram</button></td>
                        </tr>
                    </tbody>
                </table>


